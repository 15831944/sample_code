        MENUITEM "体积测量",                        IDM_Tiji
        MENUITEM "水淹分析",                        ID_Shuiyan
        MENUITEM "水库体积",                        IDM_SHUiTiTIJI
        MENUITEM "土方测量",                        IDM_Shantitiji
        MENUITEM "断面查询",                        IDM_Hjm
        MENUITEM "等高线图",                        IDM_Dgx
        MENUITEM "通透判定",                        IDM_Tongtoupanding
    END
    POPUP "特效(&S)"
    BEGIN
        POPUP "雨"
        BEGIN
            MENUITEM "开",                           ID_RAIN
            MENUITEM "关",                           ID_RAIN_DEL
        END
        POPUP "雪"
        BEGIN
            MENUITEM "开",                           ID_SNOW
            MENUITEM "关",                           ID_SNOW_DEL
        END
        POPUP "雾"
        BEGIN
            MENUITEM "开",                           ID_SE_FOG
            MENUITEM "关",                           ID_SE_FOG_DEL
        END
    END
    POPUP "帮助(&H)"
    BEGIN
        MENUITEM "关于VR-GIS(&A)...",             ID_APP_ABOUT
    END
END


