IDR_MAINFRAME MENU 
BEGIN
    POPUP "文件(&F)"
    BEGIN
        MENUITEM "退出(&X)",                      ID_APP_EXIT
    END
    POPUP "GIS功能(&G)"
    BEGIN
        POPUP "阴影"
        BEGIN
            MENUITEM "开",                           ID_ShadowON
            MENUITEM "关",                           id_shadowoff
        END
        POPUP "鹰眼"
        BEGIN
            MENUITEM "设置鹰眼",                        ID_SETHAWK
            MENUITEM "生成图像",                        ID_GENHAWKIMG
            MENUITEM "开启鹰眼",                        ID_HAWKEYEON
            MENUITEM "关闭鹰眼",                        ID_HAWKEYEOFF
        END
        MENUITEM "坐标查询",                        IDM_Zuobiao
        MENUITEM "距离测量",                        IDM_Juli
